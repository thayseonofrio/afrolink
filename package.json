{
  "name": "afrolink",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:client": "webpack-dev-server --mode development",
    "build:client": "webpack --mode development",
    "build:client:prod": "webpack --mode production",
    "build": "npm-run-all clean transpile",
    "clean": "rimraf server/dist",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "server": "node dist/index.js",
    "start:server:dev": "npm run dev",
    "start:server": "npm run prod",
    "test": "jest",
    "transpile": "babel ./server --out-dir dist --ignore 'node_modules/**/*.js'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@testing-library/react": "^10.4.6",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.9.18",
    "npm-run-all": "^4.1.5",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^5.11.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "eslint": "^7.2.0",
    "eslint-plugin-react": "^7.20.3",
    "file-loader": "^6.0.0",
    "jest": "^26.0.1",
    "jest-transform-css": "^2.0.0",
    "rimraf": "^3.0.2",
    "style-loader": "^0.23.0",
    "supertest": "^4.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "dist/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/client/src/setuptests.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
    }
  }
}
