    "mongoose": "^5.9.18",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "jest": "^26.0.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": ["<rootDir>/dist/"],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "dist/"
